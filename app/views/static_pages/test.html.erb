<% provide(:title, "Test") %>
<h1>Sign in</h1>

<div class="row">
  <div class="col-lg-6 col-md-6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :netid, "NetID" %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in", class: "btn btn-md btn-default" %>
    <% end %>
  </div>
</div>


